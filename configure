s,@STDC_NORETURN@,$STDC_NORETURN,;t t
s,@LIBOBJS@,$LIBOBJS,;t t
s,@ALLOCA@,$ALLOCA,;t t
s,@ALLOCA_H@,$ALLOCA_H,;t t
s,@GETOPT_H@,$GETOPT_H,;t t
s,@SYSEXITS_H@,$SYSEXITS_H,;t t
